{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "node": "20.18.1",
      "env": {
        "EXPO_USE_METRO_WORKSPACE_ROOT": "1"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "node": "20.18.1",
      "env": {
        "EXPO_USE_METRO_WORKSPACE_ROOT": "1"
      }
    },
    "production": {
      "ios": {
        "distribution": "store"
      },
      "android": {
        "distribution": "store"
      },
      "node": "20.18.1",
      "env": {
        "EXPO_USE_METRO_WORKSPACE_ROOT": "1"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6758531048"
      },
      "android": {
        "serviceAccountKeyPath": "./google-service-account.json",
        "track": "internal"
      }
    }
  }
}
